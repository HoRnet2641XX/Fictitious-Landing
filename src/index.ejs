                <p>龍安寺</p>
              </div>
            </div>
          </div>
          <div class="homeContent__pickupArea">
            <img src="img/pickup-6.png" class="areaIntroduction" />
            <div class="mask">
              <div class="hover__mask">
                <p>寺田屋</p>
              </div>
            </div>
          </div>
        </article>
        <article class="homeContent__three primary-Height"></article>
        <article class="homeContent__for primary-Height"></article>
        <article class="homeContent__five primary-Height"></article>
      </section>
      <%- include('components/_footer.ejs'); %>
